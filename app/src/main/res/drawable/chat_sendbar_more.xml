<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
<item android:state_pressed="false">
    <bitmap android:gravity="center" android:src="@mipmap/chat_sendbar_more"></bitmap>
</item>
<item android:state_pressed="true">
    <layer-list>
        <item>
            <shape>
                <solid android:color="@color/bg_transparent_half"></solid>
            </shape>
        </item>
        <item>
            <bitmap android:gravity="center" android:src="@mipmap/chat_sendbar_more"></bitmap>
        </item>
    </layer-list>
</item>
</selector>